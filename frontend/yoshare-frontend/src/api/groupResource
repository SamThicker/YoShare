import request from "../../static/utils/request";

export function getGroupResources(groupId, type, classis) {
  if (!type) type = "all";
  if (!classis) classis = "";
  return request({
    url: "resource/group/" + groupId + "/" + type + "/" + classis,
    method: "get"
  });
}

export function delResource(groupId, type, classis, resource) {
  return request({
    url: "resource/group/" + groupId + "/" + type + "/" + classis,
    method: "delete",
    data: resource
  });
}
